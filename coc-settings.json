{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "ts",
    "tsx",
    "elm",
    "javascript",
    "javascriptreact",
    "svelte",
    "vue",
    "typescript",
    "typescriptreact"
  ],
  "vetur.format.defaultFormatter.html": "prettier",
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    }
  }
}
